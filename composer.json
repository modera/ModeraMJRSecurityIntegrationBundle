{
    "name": "modera/mjr-security-integration-bundle",
    "type": "symfony-bundle",
    "description": "Bundle provides integration layer which is necessary to make MJR to be security aware.",
    "license": "MIT",
    "homepage": "http://modera.org",
    "keywords": ["mjr"],
    "authors": [
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@modera.org",
            "homepage": "http://sergei.lissovski.org/"
        }
    ],
    "require": {
        "php": ">=5.6",
        "doctrine/orm": "^2.5",
        "modera/mjr-integration-bundle": "~3.1|~4.0",
        "modera/foundation-bundle": "~3.1|~4.0",
        "modera/security-bundle": "~3.1|~4.0",
        "sergeil/expander-bundle": "^1.0"
    },
    "require-dev": {
        "phake/phake": "^2.0",
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-0": { "Modera\\MJRSecurityIntegrationBundle": "" }
    },
    "target-dir": "Modera/MJRSecurityIntegrationBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
