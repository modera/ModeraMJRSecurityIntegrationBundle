{
    "name": "modera/mjr-security-integration-bundle",
    "type": "symfony-bundle",
    "description": "Bundle provides integration layer which is necessary to make MJR to be security aware.",
    "license": "MIT",
    "homepage": "http://modera.org",
    "keywords": ["mjr"],
    "authors": [
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@modera.org",
            "homepage": "http://sergei.lissovski.org/"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/orm": "^2.5",
        "modera/mjr-integration-bundle": "^4.0",
        "modera/foundation-bundle": "^4.0",
        "modera/security-bundle": "^4.0",
        "sergeil/expander-bundle": "^1.2"
    },
    "require-dev": {
        "phake/phake": "^4.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": { "Modera\\MJRSecurityIntegrationBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}
